{
  "name": "goetas-webservices/soap-client",
  "authors": [
    {
      "name": "Asmir Mustafic",
      "email": "goetas@gmail.com"
    }
  ],
  "require": {
    "php": "^5.5|^7.0",

    "psr/http-message": "^1.0",
    "php-http/httplug": "^1.0",
    "php-http/discovery": "^1.0",
    "php-http/message-factory": "^1.0.2",
    "doctrine/instantiator": "^1.0.3",

    "jms/serializer": "^1.2",
    "goetas-webservices/xsd2php-runtime": "^0.2",
    "goetas-webservices/soap-common": "^0.1@dev",

    "symfony/dependency-injection": "^2.2|^3.0"

  },
  "require-dev": {
    "symfony/config": "^2.2|^3.0",
    "symfony/yaml": "^2.1|^3.0",

    "phpunit/phpunit": "^4.8|^5.0",
    "goetas-webservices/xsd2php": "^0.1@dev",
    "goetas-webservices/wsdl2php": "^0.1.2@dev",
    "php-http/guzzle6-adapter": "^1.0",
    "php-http/message": "^1.0",

    "goetas-webservices/soap-reader": "^0.3",
    "goetas-webservices/wsdl-reader": "^0.3"
  },
  "autoload": {
    "psr-4": {
      "GoetasWebservices\\SoapServices\\SoapClient\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GoetasWebservices\\SoapServices\\SoapClient\\Tests\\": "tests",
      "GoetasWebservices\\Xsd\\XsdToPhp\\Tests\\": "vendor/goetas-webservices/xsd2php/tests",
      "GoetasWebservices\\WsdlToPhp\\Tests\\": "vendor/goetas-webservices/wsdl2php/tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  }
}
