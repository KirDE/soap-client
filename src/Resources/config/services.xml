<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="goetas.soap_client.metadata" type="collection"></parameter>
        <parameter key="goetas.soap_common.compile_for_production" type="constant">FALSE</parameter>
    </parameters>
    <services>

        <service id="goetas.soap_client.metadata_reader" synthetic="true" public="true"></service>

        <service id="logger" public="true" class="Psr\Log\NullLogger"></service>

        <service id="goetas.wsdl2php.metadata_loader.dev" public="true"
                 class="GoetasWebservices\SoapServices\SoapCommon\MetadataLoader\DevMetadataLoader">
            <argument type="service" id="goetas.wsdl2php.metadata.generator"/>
            <argument type="service" id="goetas.wsdl2php.soap_reader"/>
            <argument type="service" id="goetas.wsdl2php.wsdl_reader"/>
        </service>

        <service id="goetas.wsdl2php.metadata_loader.array" public="true"
                 class="GoetasWebservices\SoapServices\SoapCommon\MetadataLoader\ArrayMetadataLoader">
            <argument>%goetas.soap_client.metadata%</argument>
        </service>

        <service id="goetas.wsdl2php.metadata.generator"
                 class="GoetasWebservices\SoapServices\SoapCommon\MetadataGenerator\MetadataGenerator">
            <argument id="goetas.xsd2php.naming_convention" type="service"/>
            <argument type="collection"/>
        </service>

    </services>
</container>
